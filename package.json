{
  "name": "klima-subgraph",
  "version": "0.0.1",
  "repository": "https://github.com/KlimaDAO/klima-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "test": "graph test",
    "create-local": "graph create --node http://127.0.0.1:8020 klima",
    "remove-local": "graph remove --node http://127.0.0.1:8020 klima",
    "watch-local": "graph deploy --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001 klima",
    "deploy-local": "graph deploy --node http://127.0.0.1:8020 --ipfs http://localhost:5001 klima",
    "deploy-staging": "graph deploy --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/ klima",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ klima"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.22.0",
    "@graphprotocol/graph-ts": "^0.22.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
